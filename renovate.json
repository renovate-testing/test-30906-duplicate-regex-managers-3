{
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["file"],
      "matchStrings": ["a=(?<currentValue>.*)"],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "colinhacks/zod"
    },
    {
      "customType": "regex",
      "fileMatch": ["file"],
      "matchStrings": ["b=(?<currentValue>.*)"],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "redis/redis"
    }
  ]
}
